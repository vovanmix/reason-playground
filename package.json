    "babel-plugin-transform-react-display-name": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "node-sass": "^4.7.2",
    "react-select": "^1.2.1",
    "reason-react": "^0.3.1"
  },
  "devDependencies": {
    "bs-platform": "^2.1.0",
    "bsb-js": "^1.0.2",
